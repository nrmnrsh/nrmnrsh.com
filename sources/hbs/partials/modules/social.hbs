<div class="content content-social">
	<div>
		<h2>{{context.headline}}</h2>

		<ul>
			{{#each context.links}}
				<li class="is-type-{{this.type}}">
					<a
						href="{{this.href}}"
						title="Follow me on {{this.label}}"
						target="_blank"
						rel="noopener bookmark">
						<span>{{this.label}}</span>
					</a>
				</li>
			{{/each}}
		</ul>
	</div>
</div>
